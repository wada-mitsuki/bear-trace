    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.0.4",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "^9.1.0",
